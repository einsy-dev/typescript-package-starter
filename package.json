    "typescript": "~4.4.3",
    "esbuild": "~0.23.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@einsy-dev"
  }
