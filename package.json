{
  "name": "@einsy-dev/package",
  "version": "1.1.1",
  "description": "TypeScript boilerplate for NPM or Github Packages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "esbuild src/index.ts  --bundle --outfile=bin/index.cjs --format=cjs --platform=node"
  },
